#!/bin/sh

# This is an example for using the free "engin" project
# Use /scratch for runs

#PBS -V
#PBS -A engin_flux
#PBS -l qos=flux
#PBS -q flux
#PBS -N SWMF
#PBS -l procs=32          # There are only 100 cores
#PBS -l walltime=8:00:00

# cd to job submission directory
cd $PBS_O_WORKDIR

# get queue status
qstat -u $USER -n

# run job
mpirun ./SWMF.exe > runlog_`date +%y%m%d%H%M`

